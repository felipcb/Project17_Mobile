pipeline{
    agent any

    stages{
        stage('Clonar repositório'){
            steps{
                git branch: 'master', url: 'https://github.com/felipcb/Project17_Mobile.git'
            }
        }
        
        stage('Instalar dependências') {
            steps {
               powershell 'npm install -y' 
            }
        }

        stage('Iniciar testes') {
            steps {
                powershell 'npm run wdio'
            }
        }
    }
}